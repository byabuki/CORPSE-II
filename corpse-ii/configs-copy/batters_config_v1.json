{
    "base_projection": "thebatx",
    "categories": {
        "playerid": {
            "noValuation": true,
            "destinationColumn": "player_id"
        },
        "PlayerName": {
            "noValuation": true,
            "destinationColumn": "name"
        },
        "PA": {
            "noValuation": true,
            "isRateStat": false
        },
        "AB": {
            "noValuation": true,
            "isRateStat": false
        },
        "HR": {
            "isPositiveStat": true,
            "isRateStat": false
        },
        "BB": {
            "isPositiveStat": true,
            "isRateStat": false
        },
        "TB": {
            "isPositiveStat": true,
            "isRateStat": false,
            "formula": "SLG * AB"
        },
        "OBP": {
            "isPositiveStat": true,
            "isRateStat": true,
            "denominator": "PA"
        },
        "SLG": {
            "isPositiveStat": true,
            "isRateStat": true,
            "denominator": "AB"
        },
        "SB": {
            "isPositiveStat": true,
            "isRateStat": false
        }
    }
}
